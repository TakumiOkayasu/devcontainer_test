{
	"name": "Debian Unstable Dev Container",
	"dockerComposeFile": "compose.yaml",
	"service": "app",
	"workspaceFolder": "/workspace",
	"customizations": {
		"vscode": {
			"settings": {
				"terminal.integrated.shell.linux": "bash"
			},
			"extensions": [
				"esbenp.prettier-vscode",
				"ms-azuretools.vscode-docker",
				"dbaeumer.vscode-eslint"
			]
		}
	},
	"remoteUser": "vscode"
}